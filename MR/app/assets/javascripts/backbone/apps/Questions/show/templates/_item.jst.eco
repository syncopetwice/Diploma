<div class="item">
  <div class="question">
    <strong><%= @question %></strong>. <%= @title %>
  </div>
  <div class="options">
    <% for option in @options: %>
      <label for="values">
        <input type="radio" class="radio" name="values-<%= @question %>" value=<%= option %>><%= option %>
      </label>
    <% end %>
  </div>
</div>